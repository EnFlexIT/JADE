# Makefile for Agent Development Kit project - src directory

# The following targets are not file names
.PHONY: all clean

JC = javac
JFLAGS =
TARGETS =

all: 
	$(JC) $(JFLAGS) $(TARGETS)
clean:
	rm -f `find . -name '*~' -print`
	rm -f `find . -name '#*#' -print`
	rm -f `find . -name '*.class' -print`


